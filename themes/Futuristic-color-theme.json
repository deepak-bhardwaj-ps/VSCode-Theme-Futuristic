{
  "name": "Futuristic Dark",
  "type": "dark",
  "author": "Deepak Bhardwaj",
  "publisher": "Deepak Bhardwaj",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#1A5461"
      }
    },

    {
      "name": "[JAVASCRIPT] - Numbers",
      "scope": "source.js constant",
      "settings": {
        "foreground": "#CBC53D"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#8AE2F4"
      }
    },
    {
      "name": "String",
      "scope": ["string.quoted"],
      "settings": {
        "foreground": "#8e6cba"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#8e6cba"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#CBC53D"
      }
    },
    {
      "name": "Language Constant (`true`, `false, `null`, etc.)",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#77C4D6"
      }
    },

    {
      "name": "Operator (`+`, `-`, `!`, `typeof`, `instanceof`, etc.)",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#77C4D6"
      }
