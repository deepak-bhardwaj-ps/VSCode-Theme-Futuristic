  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#1F7A8C",
        "fontStyle": "italic"
      }
    },

    {
      "name": "[JAVASCRIPT] - Numbers",
      "scope": "source.js constant",
      "settings": {
        "foreground": "#CBC53D"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#8AE2F4"
      }
    },
    {
      "name": "String",
      "scope": ["string.quoted"],
      "settings": {
        "foreground": "#C7BDFF"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#C7BDFF"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#CBC53D"
      }
    },
    {
      "name": "Language Constant (`true`, `false, `null`, etc.)",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#CBC53D"
      }
    },

    {
      "name": "Operator (`+`, `-`, `!`, `typeof`, `instanceof`, etc.)",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#F0BD6A"
      }
    },
    {
      "name": "Destructuring Punctuation (`[...] = [...]`, `{...} = {...}`)",
      "scope": [
        "punctuation.definition.binding-pattern",
        "meta.import punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#77C4D6"
      }
    },
    {
      "name": "Control Flow (`if`, `else`, `switch`, `return`, etc.)",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#1AFF3C"
      }
    },
    {
      "name": "Storage Type (`function`, `class`, `int`, `bool`, etc.)",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#FFB700"
      }
    },
    {
      "name": "Storage Modifier (`public`, `private`, `static`, etc.)",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#34945E"
      }
    },
    {
      "name": "Language Variable (`this`, `self`, `super`, etc.)",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#FEFBFB"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "fontStyle": "",
        "foreground": "#34945E"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.other",
      "settings": {
        "fontStyle": "",
        "foreground": "#FC9C9D"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class, entity.name.type",
      "settings": {
        "fontStyle": "",
        "foreground": "#F49227"
      }
    },
    {
      "name": "Class Variable",
      "scope": "punctuation.separator.variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#EFEFEF"
      }
    },
    {
      "name": "Language Method/Function (`setTimeout`, `printf`, etc.)",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#DBA80A"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "meta.tag punctuation.definition.string",
      "settings": {
        "fontStyle": "",
        "foreground": "#DBA80A"
      }
    },
    {
      "name": "Meta Brace",
